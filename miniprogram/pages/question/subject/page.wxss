/* miniprogram/pages/question/subject/page.wxss */
.swiper-box{
  position: fixed;
  top: 0;
  width: 100%;
  height: 90vh;
}
swiper-item{
  overflow: auto;
}
.opstionAnswer{
  position:relative;
  margin: 20rpx 50rpx;
}
label{
  display:block;
  color: #999999;
  margin-bottom: 30rpx;
}
.questionTitle{
  font-size: 26rpx;
  line-height: 42rpx;
  margin:20rpx 25rpx;
  height: 40vh;
  overflow: hidden;
}
.questionTitle-ellipsis{
  height: auto;
}
.questionTitle .questionTypeName{
  color: #333333;
  font-size: 34rpx;
  font-weight: bold;
  margin-left:-16rpx;
}
.questionTitle .questionTitle-content{
  position: relative;
}
.ellipsis{
  position:relative;
  text-align:right;
  color:#1090B3;
  font-size:28rpx;
  margin:0 25rpx;
  vertical-align:middle;
}
.ellipsis text{
  margin-right: 10rpx ;
}
.ellipsis image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.serial-number{
  position: relative;
  margin:0 25rpx;
  font-size:28rpx;
  float: right;
}
.serial-number{
  position: relative;
  font-size: 22rpx;
  color: #26292C;
}



.table{
  position: relative;
  display: table;
  border-collapse: separate;
  border-spacing: 5px 15px;
  width: 100%;
}

.table-row{
  position: relative;
  display: table-row;
  width: 100%;
}
.table-cell{
  position: relative;
  display: table-cell;
  vertical-align: middle;
  width: 10%;
}
.table-cell:nth-child(2n){
  position: relative;
  width:  90%;
}
.cirle {
  position: relative;
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
  color: #26292C;
  line-height: 52rpx;
  font-size: 26rpx;
  text-align: center;
  background-color: #EDEFF2;
  border-radius: 50%;
  margin-right: 25rpx;
}
.check .cirle{
  color: #fff;
  background-color: #15C29D;
}

.look_up .check .cirle{
  color: #fff;
  background-color: #F91020;
}

.look_up .answerCheck .cirle{
  color: #fff;
  background-color: #15C29D;
}

.checkbox-btn{
  position: relative;
  height: 76rpx;
  line-height: 76rpx;
  width: 55%;
  margin:auto;
  margin-top: 120rpx;
  text-align: center;
  color: #FFF;
  background-color: #FF3233;
  border-radius: 38rpx;
}

.text-line::after{
  position: absolute;
  content: "";
  width: 85rpx;
  height: 2rpx;
  background-color: #A9ACB0;
  right: 66rpx;
  margin-top: 8px;
}
.text-line::before{
  position: absolute;
  content: "";
  width: 85rpx;
  height: 2rpx;
  background-color: #A9ACB0;
  left: 66rpx;
  margin-top: 8px;
}
.obAnswer{
  position: relative;
  display: flex;
}
.obAnswer .rightAnswer{
  position: relative;
  flex: 1;
  text-align: center;
}
.obAnswer .rightAnswer-title{
  position: relative;
  color: #26292C;
  font-size: 24rpx;
  padding: 15rpx;
}
.obAnswer .myAnswer{
  position: relative;
  flex: 1;
  text-align: center;
}
.analyzeWord-title{
  position: relative;
  text-align: center;
  width: 100%;
  line-height: 80rpx;
  font-size: 26rpx;
}
.analyzeWord{
  position: relative;
  font-size: 22rpx;
  line-height: 30rpx;
}
.analyzeWord-box{
  position: relative;
  margin-top: 60rpx;
  display: flex;
  padding-bottom:20rpx;
}
.analyzeWord-box .analyze_control{
  position: relative;
  flex: 1;
}
.analyze_control button{
  text-align: center;
  width: 240rpx;
  height: 56rpx;
  border-radius: 30rpx;
  background-color: #EEEEEE;
  color: #333333;
  font-size: 22rpx;
  margin: auto;
}
.situation{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  min-height: 300rpx;
  max-height: 100%;
  overflow-y: auto;
  -webkit-overflow-scrolling: auto;
  z-index: -1;
}
.float-question{
  position: relative;
  /* transition: all 1s; */
}
.float-question .question-top{
  position: absolute;
  width: 100%;
  height: 80rpx;
  top: -75rpx;
}
.question-box{
  position: relative;
  overflow-y: auto;
  background: #FFF;
  height: 100%;
}
/**
 *  底部菜单
 */
.nav-tab{
  width: 100%;
  height: 10vh;
  position: fixed;
  display: flex;
  bottom: 0;
  box-shadow:0px -1px 8px 0px rgba(221,221,221,0.98);
  background-color: #FFF;
  align-items:center;
  justify-content:center;
}

.menu{
  position: relative;
  display: inline-block;
  flex: 1;
  text-align: center;
}
.menu .icon-image{
  position: relative;
  display: block;
  width: 36rpx;
  height: 36rpx;
  margin: auto;
  margin-bottom: 10rpx;
}
.menu .menu-name{
  position: relative;
  font-size: 24rpx;
  color: #A9ACB0;
  margin-top: 10rpx;
}
.gotoHome{
  position: relative;
  border: none;
  margin: 0;
  padding: 0;
  background:linear-gradient(90deg,rgba(245,67,70,1) 0%,rgba(249,129,131,1) 100%);
  box-shadow:0px 2px 8px 0px rgba(251,53,57,0.51);
  width: 100%;
  height: 100%;
  line-height: 10vh;
  text-align: center;
  color: #FFFFFF;
  font-size: 28rpx;
}
.gotoHome .boldtext{
  font-size: 36rpx;
  font-weight: bold;
}

/* 交卷弹框 start*/
.modelSubmitpaper{
  position: fixed;
  display: flex;
  width: 100%;
  height: 100vh;
  justify-content:center;
  align-items:center;
  z-index: 10000;
}
.modelSubmitpaper .submit-box{
  position:  relative;
  width: 462rpx;
  height: 500rpx;
  z-index: 99;
}
.submit-box .submit-bg-tips{
  position: relative;
  display: flex;
}
.modelSubmitpaper .floatbtn{
  position:absolute;
  width: 100%;
  top: 247rpx;
  height: 254rpx;
}
.modelSubmitpaper .floatbtn .remarks{
  position:relative;
  font-size:26rpx;
  height: 140rpx;
  color: #26292C;
  text-align: center;
}
.remarks .back-tips{
  position: relative;
  line-height: 160rpx;
}
.remarks .submit-tips{
  position: relative;
  
}
.modelSubmitpaper .submit-btn{
  position: absolute;
  display: flex;
  width: 100%;
  bottom: 0;
}
.modelSubmitpaper .submit-btn .btn-mt{
  display: inline-block;
  flex: 1;
  height: 56rpx;
  border-radius: 28rpx;
  color: #FFFFFF;
  padding: 0;
  font-size:28rpx;
  line-height: 56rpx;
  margin: 30rpx 30rpx;
}
.altremodel{
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #000000;
  opacity: 0.3;
  z-index: 98;
}