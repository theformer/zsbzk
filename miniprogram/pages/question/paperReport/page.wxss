/* miniprogram/pages/question/subjectSubmit/page.wxss */  
.modelReport{
    position: relative;
    width: 100%;
    height: 100vh;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
  }
  .modelReport .report_top_bg{
    position: absolute;
    width: 100%;
  }
  .modelReport .report_analys{
    position: relative;
    margin-top: 10vh;
    width: 100%;
    z-index: 100;
  }
  .report_analys .report_analys_box{
    position: relative;
    margin: 40rpx;
    height: 450rpx;
    text-align: center;
    color: #333333;
    background-color: #FFFFFF;
    box-shadow:0px 4px 10px 1px #E6E6E6;
    border-radius: 10rpx;
    padding:50rpx
  }
  .report_analys .report_accuracy{
    position: relative;
    font-size: 30rpx;
  }
  .report_analys .report_accuracy text{
    position: relative;
    font-size: 80rpx;
    font-weight: bold;
  }
  .report_analys .report_text{
    position: relative;
    font-size: 28rpx;
  }
  .report_analys .report_detail{
    position: relative;
    margin-top: 60rpx;
    display: flex;
  }
  .report_detail .report_detail_li{
    position: relative;
    flex: 1;
    text-align: center;
  }
  .report_detail_li .report_detail_data{
    position: relative;
    color: #26292C;
    font-size: 30rpx;
    font-weight: bold;
  }
  .report_detail_li .report_detail_text{
    position: relative;
    color: #26292C;
    font-size: 24rpx;
    margin-top: 20rpx;
  }
  .report_control_button{
    position: relative;
    margin-top: 40rpx;
    display: flex;
  }
  .report_control_button .report_control{
    position: relative;
    flex: 1;
  }
  .report_control_button .report_lookAnswer_btn{
    text-align: center;
    height: 56rpx;
    border-radius: 30rpx;
    background-color: #E70012;
    color: #FFFFFF;
    font-size: 22rpx;
    margin: 0 80rpx;
  }
  .report_control_button .report_share_btn{
    position: relative;
    flex: 1;
    text-align: center;
    width: 240rpx;
    height: 56rpx;
    border-radius: 30rpx;
    border:1rpx solid #FC4D30;
    color: #FC4D30;
    font-size: 22rpx;
    margin: auto;
  }
  .report_other_info{
    position: relative;
    padding: 0 30rpx;
    background: #FFFFFF;
  }
  .report_other_info .report_info_item{
    position: relative;
    width: 50%;
    vertical-align:middle;
    display: inline-block;
  }
  .report_info_item .report_hot{
    position: relative;
    display: inline-block;
    width: 52rpx;
    height: 26rpx;
  }
  .report_info_item .report_hot_title{
    position: absolute;
    top: 10rpx;
    width: 55rpx;
    height: 26rpx;
    text-align: center;
    font-size: 20rpx;
    line-height: 26rpx;
    vertical-align:middle;
    right: 20rpx;
    color: #FFFFFF;
  }
  .report_info_item .report_item_img{
    position: relative;
    width: 56rpx;
    height: 56rpx;
    display: inline-block;
    margin-right: 20rpx;
  }
  .report_info_item .report_item_content{
    position: relative;
    display: inline-block;
  }
  .report_item_content .item_content_title{
    position: relative;
    color: #26292C;
    font-size: 28rpx;
    line-height: 50rpx;
  }
  .report_item_content .item_content_tips{
    position: relative;
    color: #A9ACB0;
    font-size: 22rpx;
    line-height: 50rpx;
  }
  .report_idea{
    position: relative;
  }
  .report_idea .report_idea_title{
    position: relative;
    color: #26292C;
    font-size: 28rpx;
  }
  .report_idea .report_idea_content{
    position: relative;
    color: #26292C;
    font-size: 24rpx;
  }
 
  .nav-image{
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
  }

  .nav-wrap .nav-title{
    top: auto;
    line-height:normal;
  }