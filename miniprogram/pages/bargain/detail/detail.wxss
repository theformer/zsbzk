    height: 100%;
    display: flex;
    justify-content: center;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 99998;
    opacity: 0.7;
}

.video-modalDlg {
    width: 70%;
    height: auto;
    left: 0;
    position: fixed;
    z-index: 99999;
    margin: 45vw 12%;
    background-color: #fff;
    border-radius: 16rpx;
    display: flex;
    justify-content: center;
