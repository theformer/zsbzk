/* 报名系统弹框样式  start  */

.zan-dialog__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
    background: rgb(0, 0, 0); /*设置阴影半透明背景如： background: rgba(0, 0, 0, 0.4);*/
    display: none;
    opacity: 0.3;
}

.zan-dialog__container {
    position: fixed;
    top: 200rpx;
    width: 80%;
    /* height: 350rpx;弹窗布局高，与下面弹出距离transform有关 */
    margin-left: 10vw;
    background: #fff;
    transform: translateY(400%); /*弹框弹出距离，与弹框布局高度有关，如300%表示弹起距离为3倍弹窗高度*/
    transition: all 0.4s ease;
    z-index: 12;
    border-radius: 20rpx;
    /* box-shadow: 0px 3px 3px 2px gainsboro;弹框的悬浮阴影效果，如不需要可注释该行 */
}

.zan-dialog--show .zan-dialog__container {
    transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
    display: block;
}

.closeBtn {
    background: #ff5a61;
    width: 60%;
    display: block;
    margin: 10px auto;
    height: 40px;
    line-height: 40px;
    border-radius: 20px;
    webkit-border-radius: 20px;
    color: #fff;
    outline: none;
    border: 0;
    letter-spacing: 2px;
    font-size: 15px;
}

.scroll-view {
    height: 240px;
    overflow: auto;
}

.scroll-view view {
    font-size: 14px;
    color: #333;
    width: 90%;
    background: #fff;
    margin: 0 auto;
    line-height: 2;
}

/* 报名系统弹框样式 end  */